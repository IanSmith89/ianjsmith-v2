{
	"name": "ianjsmith",
	"version": "2.0.1",
	"description": "My portfolio site",
	"main": "webpack.config.js",
	"scripts": {
		"build": "rimraf ./dist && NODE_ENV=production webpack --config webpack.config.js",
		"start": "NODE_ENV=production node server/index.js",
		"build:dev": "rimraf ./dist && NODE_ENV=development webpack --config webpack.config.js",
		"start:dev": "NODE_ENV=development node server/index.js",
		"deploy:prod": "git push heroku master",
		"deploy:staging": "git push heroku-staging staging:master",
		"heroku-postbuild": "npm rebuild node-sass && yarn build",
		"knex": "knex",
		"migrate:latest:test": "NODE_ENV=test knex migrate:latest",
		"migrate:latest": "knex migrate:latest",
		"migrate:rollback:test": "NODE_ENV=test knex migrate:rollback",
		"migrate:rollback": "knex migrate:rollback",
		"nf": "nf start",
		"prettier": "prettier --write --config .prettierrc \"**/*.{js,jsx,ts,tsx,json,css,scss}\"",
		"resetdb": "yarn migrate:rollback && yarn migrate:latest && yarn seed",
		"resetdb:heroku:prod": "heroku run --app ianjsmith npm run resetdb",
		"resetdb:heroku:staging": "heroku run --app ianjsmith-staging npm run resetdb",
		"resetdb:test": "yarn migrate:rollback:test && yarn migrate:latest:test && yarn seed:test",
		"seed:test": "NODE_ENV=test knex seed:run",
		"seed": "knex seed:run",
		"test:server": "mocha test/**/*.test.js --env test",
		"test": "yarn test:server",
		"test:push": "yarn test && git push origin HEAD"
	},
	"eslintConfig": {
		"extends": "./eslint.js"
	},
	"keywords": [
		"React",
		"React-Router 4",
		"MobX",
		"Webpack 2",
		"ES6",
		"ES7"
	],
	"author": "Ian Smith",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.21.2",
		"babel-polyfill": "^6.26.0",
		"body-parser": "^1.17.1",
		"bookshelf": "^0.10.3",
		"bookshelf-camelcase": "^1.1.4",
		"classnames": "^2.2.5",
		"compression": "^1.6.2",
		"cookie-parser": "^1.4.3",
		"dotenv": "^4.0.0",
		"express": "^4.15.2",
		"gsap": "3.6.0",
		"joi": "^10.2.2",
		"knex": "^0.21.1",
		"lazy-route": "^1.0.7",
		"mobx": "^5.9.0",
		"mobx-react": "^5.4.3",
		"morgan": "^1.8.1",
		"node-sass": "7.0.0",
		"pg": "^8.0.3",
		"react": "^16.8.1",
		"react-dom": "^16.8.1",
		"react-image-lightbox": "^5.1.0",
		"react-router-dom": "^4.0.0",
		"react-scroll": "^1.5.4",
		"react-transition-group": "^2.5.0"
	},
	"devDependencies": {
		"@babel/core": "^7.17.5",
		"@babel/plugin-proposal-decorators": "^7.17.2",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"autoprefixer": "^7.1.2",
		"babel-loader": "^8.2.3",
		"babel-plugin-autobind-class-methods": "^5.0.1",
		"chai": "^3.5.0",
		"css-loader": "^6.7.0",
		"eslint": "^5.6.1",
		"html-webpack-plugin": "^5.5.0",
		"mocha": "^3.2.0",
		"postcss-loader": "^6.2.1",
		"precss": "^2.0.0",
		"prettier": "^1.14.3",
		"react-hot-loader": "^3.0.0-beta.2",
		"resolve-url-loader": "^5.0.0",
		"rimraf": "^2.5.4",
		"sass-loader": "^12.6.0",
		"style-loader": "^3.3.1",
		"supertest": "^3.0.0",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.7.4",
		"whatwg-fetch": "^1.0.0"
	},
	"repository": {
		"url": "https://github.com/IanSmith89/ianjsmith-v2.git",
		"type": "git"
	},
	"engines": {
		"node": "16.14.0",
		"npm": "8.3.1",
		"yarn": "1.22.4"
	}
}
