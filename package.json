{
	"name": "ianjsmith",
	"version": "2.0.1",
	"description": "My portfolio site",
	"main": "webpack.config.js",
	"scripts": {
		"build": "rimraf ./dist && NODE_ENV=production webpack --config webpack.config.js",
		"start": "NODE_ENV=production node server/index.js",
		"build:dev": "rimraf ./dist && NODE_ENV=development webpack --config webpack.config.js",
		"start:dev": "NODE_ENV=development node server/index.js",
		"deploy:prod": "git push heroku master",
		"deploy:staging": "git push heroku-staging staging:master",
		"heroku-postbuild": "npm rebuild node-sass && yarn build",
		"knex": "knex",
		"migrate:latest:test": "NODE_ENV=test knex migrate:latest",
		"migrate:latest": "knex migrate:latest",
		"migrate:rollback:test": "NODE_ENV=test knex migrate:rollback",
		"migrate:rollback": "knex migrate:rollback",
		"nf": "nf start",
		"prettier": "prettier --write --config .prettierrc \"**/*.{js,jsx,ts,tsx,json,css,scss}\"",
		"resetdb": "yarn migrate:rollback && yarn migrate:latest && yarn seed",
		"resetdb:test": "yarn migrate:rollback:test && yarn migrate:latest:test && yarn seed:test",
		"seed:test": "NODE_ENV=test knex seed:run",
		"seed": "knex seed:run",
		"test:server": "mocha test/**/*.test.js --env test",
		"test": "yarn test:server",
		"test:push": "yarn test && git push origin HEAD"
	},
	"eslintConfig": {
		"extends": "./eslint.js"
	},
	"keywords": [
		"React",
		"React-Router 4",
		"MobX",
		"Webpack 2",
		"ES6",
		"ES7"
	],
	"author": "Ian Smith",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.15.0",
		"babel-polyfill": "^6.26.0",
		"body-parser": "^1.17.1",
		"bookshelf": "^0.10.3",
		"bookshelf-camelcase": "^1.1.4",
		"classnames": "^2.2.5",
		"compression": "^1.6.2",
		"cookie-parser": "^1.4.3",
		"dotenv": "^4.0.0",
		"express": "^4.15.2",
		"joi": "^10.2.2",
		"knex": "^0.12.8",
		"lazy-route": "^1.0.7",
		"mobx": "^2.5.2",
		"mobx-react": "^3.5.7",
		"moment": "^2.15.1",
		"morgan": "^1.8.1",
		"node-sass": "^3.10.1",
		"pg": "^6.1.5",
		"react": "^15.3.2",
		"react-dom": "^15.3.2",
		"react-router-dom": "^4.0.0",
		"react-scroll": "^1.5.4",
		"react-transition-group": "^2.5.0"
	},
	"devDependencies": {
		"autoprefixer": "^7.1.2",
		"babel-core": "^6.26.0",
		"babel-loader": "^6.3.2",
		"babel-plugin-transform-async-to-generator": "^6.16.0",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.16.0",
		"babel-preset-stage-0": "^6.16.0",
		"chai": "^3.5.0",
		"css-loader": "^0.25.0",
		"eslint": "^5.6.1",
		"extract-text-webpack-plugin": "^2.0.0-beta.4",
		"html-webpack-plugin": "^2.30.1",
		"image-webpack-loader": "^2.0.0",
		"mocha": "^3.2.0",
		"postcss-loader": "^0.13.0",
		"precss": "^2.0.0",
		"prettier": "^1.14.3",
		"react-hot-loader": "^3.0.0-beta.2",
		"resolve-url-loader": "^1.6.0",
		"rimraf": "^2.5.4",
		"sass-loader": "^4.0.2",
		"style-loader": "^0.13.1",
		"supertest": "^3.0.0",
		"url-loader": "^0.5.7",
		"webpack": "2.2.1",
		"webpack-dev-server": "2.1.0-beta.8",
		"whatwg-fetch": "^1.0.0"
	},
	"repository": {
		"url": "https://github.com/IanSmith89/ianjsmith-v2.git",
		"type": "git"
	},
	"engines": {
		"node": "8.12.0",
		"npm": "6.4.1",
		"yarn": "1.12.0"
	}
}
