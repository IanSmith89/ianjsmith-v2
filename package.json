{
    "name": "ianjsmith",
    "version": "2.0.0",
    "description": "My portfolio site",
    "main": "webpack.config.js",
    "scripts": {
        "start": "node server/server.js",
        "start:dev": "env NODE_ENV=development node server/server.js",
        "build:dev": "rimraf ./dist && webpack --config webpack.config.js",
        "build:prod": "rm -rf ./dist && webpack --config webpack.config.production.js",
        "heroku-postbuild": "npm rebuild node-sass && npm run build:prod",
        "knex": "knex",
        "migrate:latest:test": "env NODE_ENV=test knex migrate:latest",
        "migrate:latest": "knex migrate:latest",
        "migrate:rollback:test": "env NODE_ENV=test knex migrate:rollback",
        "migrate:rollback": "knex migrate:rollback",
        "nf": "nf start",
        "publish": "git push heroku master",
        "publish:staging": "git push heroku-staging staging:master",
        "resetdb": "npm run migrate:rollback && npm run migrate:latest && npm run seed",
        "resetdb:test": "npm run migrate:rollback:test && npm run migrate:latest:test && npm run seed:test",
        "seed:test": "env NODE_ENV=test knex seed:run",
        "seed": "knex seed:run",
        "test:server": "mocha test/**/*.test.js --env test",
        "test": "npm run test:server",
        "test:push": "npm test && git push origin HEAD"
    },
    "keywords": ["React", "React-Router 4", "MobX", "Webpack 2", "ES6", "ES7"],
    "author": "Ian Smith",
    "license": "MIT",
    "dependencies": {
        "autoprefixer": "^7.1.2",
        "axios": "^0.15.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^6.3.2",
        "babel-plugin-transform-async-to-generator": "^6.16.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-react": "^6.16.0",
        "babel-preset-stage-0": "^6.16.0",
        "body-parser": "^1.17.1",
        "bookshelf": "^0.10.3",
        "bookshelf-camelcase": "^1.1.4",
        "chai": "^3.5.0",
        "classnames": "^2.2.5",
        "compression": "^1.6.2",
        "cookie-parser": "^1.4.3",
        "css-loader": "^0.25.0",
        "dotenv": "^4.0.0",
        "express": "^4.15.2",
        "extract-text-webpack-plugin": "^2.0.0-beta.4",
        "faker": "^3.1.0",
        "html-webpack-plugin": "^2.30.1",
        "http-server": "^0.9.0",
        "humps": "^2.0.0",
        "image-webpack-loader": "^2.0.0",
        "include-media": "^1.4.9",
        "joi": "^10.2.2",
        "knex": "^0.12.8",
        "lazy-route": "^1.0.7",
        "mobx": "^2.5.2",
        "mobx-react": "^3.5.7",
        "mobx-react-devtools": "^4.2.6",
        "mocha": "^3.2.0",
        "moment": "^2.15.1",
        "morgan": "^1.8.1",
        "node-sass": "^3.10.1",
        "package": "^1.0.1",
        "pg": "^6.1.5",
        "postcss-loader": "^0.13.0",
        "precss": "^2.0.0",
        "react": "^15.3.2",
        "react-dom": "^15.3.2",
        "react-hot-loader": "^3.0.0-beta.2",
        "react-router-dom": "^4.0.0",
        "resolve-url-loader": "^1.6.0",
        "rimraf": "^2.5.4",
        "sass-loader": "^4.0.2",
        "style-loader": "^0.13.1",
        "supertest": "^3.0.0",
        "url-loader": "^0.5.7",
        "webpack": "2.2.1",
        "webpack-dev-server": "2.1.0-beta.8",
        "whatwg-fetch": "^1.0.0"
    },
    "repository": {
        "url": "https://github.com/IanSmith89/ianjsmith-v2.git",
        "type": "git"
    },
    "engines": {
        "node": "8.4.0"
    }
}
